#TicketList{
    text-align: center;
    img{
        width: 100%;
        max-width: 200px;
    }
    td p{
        margin: 0px;
    }
    table, tr,td{
        border: none;
        padding: 0px;
    }
    .qrcode{
        font-weight: bold;
        font-size: 20px;
        margin-bottom: 0px;
    }
    .box{
        width: 100%;
        max-width: 250px;
    }
}
  #EventList{
      td p{
          margin: 0px;
      }
      table, tr,td{
          border: none;
          padding: 0px;
      }
    .box{
        position:relative;
        overflow: hidden;
        max-width: 300px;
        margin: auto;
        img{
            position: absolute;
            top: 0px;
            right: 0;
            left: 0;
            z-index: 0;
            max-width: 300px;
        }
        hr{
            color: #e7e7e7;
            height: 1px;
        }
        .barticket{
            background-color: #eeeeef;
            height: 10px;
            border-radius: 30px;
            overflow: hidden;
            .colorbar{
                background: #32bdca;
                width: 50%;
                height: 10px;
            }
        }
        .row{
            display: flex;
        }
        .total{
            margin-left: auto;
        }
    }
    .tag{
        position: relative;
        float: right;
        -webkit-transform: rotate(39deg);
        transform: rotate(39deg);
        font-size: 17px;
        right: -50px;
        width: 159px;
        z-index: 1;
    }
    .tanggalMulai{
        font-family: Roboto;
        font-size: 13px;
        font-weight: 500;
        font-style: normal;
        font-stretch: normal;
        line-height: normal;
        letter-spacing: 0.2px;
        color: #2a3fb2;
    }
    .namaevent{
        padding-top: 180px;
        font-family: Roboto;
        font-size: 22px;
        font-weight: 500;
        line-height: 1.38;
        color: #333333;
    }
    .tempatEvent{
        font-family: Roboto;
        font-size: 13px;
        line-height: 1.46;
        letter-spacing: 0.2px;
        color: #acacb0;
    }

    .box-upload{
        max-width: 300px;
        margin: auto;
        text-align: center;
        height: 300px;
        background: white;
        border-radius: 6px;
        padding: 40px 0px;
        box-shadow: 0 2px 3px rgba(10, 10, 10, 0.1), 0 0 0 1px rgba(10, 10, 10, 0.1);
    }
    
    .circle-custom{
        height: 150px;
        width: 150px;
        background: #cdcdcf;
        margin: 12px auto;
        border-radius: 80px;
        padding: 1px;
    }
}
