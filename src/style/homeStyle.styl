a{
    text-decoration: none !important;
}
.background-all{
    position: fixed;
    top: 0px;
    right: 0px;
    left: 0px;
    bottom: 0px;
    background: #f5f5f5;
}
.navbar{
    height: 60px !important;
    background: transparent;
    align-content: center;
    position: fixed !important;
    left: 0px;
    right: 0px;
    top: 0px;
    transition: 0.4s;
    max-width: 1506px;
    margin: auto;
    box-shadow: 0 5px 16px 0 rgba(127, 127, 127, 0.2);
}
.searchModif{
    width: 100%;
    .field{
        width: 80%;
        margin: auto;
        align-items: center;
        .control{
            input{
                background-color: #ececec;
                color: black;
            }
            .icon{
                color:black;
            }
            .button{
                background-image: linear-gradient(272deg, #858f99, #202326);
                border: whitesmoke;
                color: white;
            }
        }
        p{
            margin: 0px;
        }
    }
}
.navbar-brand{
    display: flex !important;
}
a.navbar-item{
    text-decoration: none;
    color:white;
}
a.navbar-item:hover{
    color:#018378;
    background-color: transparent;
    text-decoration: none;
} 

.is-register{
    text-decoration: none;
    text-align: center;
    width: 101px;
    height: 36px;
    border-radius: 4px;
    border-radius: 5px;
    background-image: linear-gradient(to right, #007eac, #0095b0);
    padding: 6px 20px;
    border:none;
    color: white !important;
}
.is-login{
    text-decoration: none;
    text-align: center;
    margin-right: 10px;
    width: auto;
    height: 36px;
    border-radius: 4px;
    border-radius: 5px;
    border:solid 1px #384a9f;
}
.is-login:hover{
    font-weight: bold;
}
.is-register:hover{
    background-image: linear-gradient(to left, #007eac, #0095b0);
}
.navbar-custom{
    background: white !important;
    transition: 0.4s;
    a.navbar-item{
        color:black;
    }

    a.navbar-item:hover{
        color:#018378;
    }
}
.navbar-blue{
    background:#384a9f !important;
    transition: 0.4s;
}
a.navbar-item:active{
    color:#19c5c5;
}
