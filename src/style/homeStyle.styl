  #OurServices{
      text-align: center;
      background-color: transparent;
      h4{
        font-size: 16px;
        opacity: 0.7;
      }
      p{
        font-size: 14px;
        opacity: 0.7;
      }
      .card-content{
          padding: 80px 60px;
      }
      .card{
          border-radius: 10px;
      }
  }

  #About{
    background-color: #f5f5f5;
    .container{
        padding: 150px 0px;
        align-items: center;
    }

    .section-subheading{
        text-align: justify;
    }
    
  }
  #Events{
    background-color: #f5f5f5;
    .titleSection{
        text-align: center;

    }
    .eventImage a .name {
        top: 0px;
        left: -100%;
        -webkit-transition: all 0.3s ease;
        -moz-transition: all 0.3s ease-in-out;
        -o-transition: all 0.3s ease-in-out;
        -ms-transition: all 0.3s ease-in-out;
        transition: all 0.3s ease-in-out;
    }

    .eventImage a .name span {
        display: block;
        padding: 10px 0;
        margin: 40px 20px 20px 20px;
        border-top-left-radius: 6px;
        border-top-right-radius: 6px;
        text-transform: uppercase;
        font-weight: normal;
        color: rgba(255,255,255,0.9);
        text-shadow: 1px 1px 1px rgba(0,0,0,0.2);
    }
    .eventImage a:hover .name{
        left: 0px;
    }

    .eventImage a,
    .eventImage a img {
        display: block;
        position: relative;
    }

    .eventImage a {
        overflow: hidden !important;
    }

    .eventImage .name {
        position: absolute;
        background: #333;
        background: rgba(75,75,75,0.7);
        width: 100%;
        height: 100%;
        margin: auto;
        border-top-left-radius: 6px;
        border-top-right-radius: 6px;
    }

    .eventImage.ng-enter {
        animation: 1s linear added;
    }
    
    .eventImage.ng-leave {
    animation: 1s linear deleted;
    }
    .event-button{
        text-align: center;
    }
  }

  #Contact{
      background-color:#384a9f;
      padding-bottom: 1px;
